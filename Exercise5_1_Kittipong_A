firstnum = int(input("First number : "))
secondnum = int(input("Second number : "))
print(firstnum,"+",secondnum,"=",firstnum+secondnum)
print(firstnum,"-",secondnum,"=",firstnum-secondnum)
print(firstnum,"*",secondnum,"=",firstnum*secondnum)
print(firstnum,"/",secondnum,"=",int(firstnum/secondnum))
